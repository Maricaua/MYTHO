{
  "expo": {
    "name": "mytho",
    "scheme": "mytho",
    "slug": "mytho",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SplashScene(Mytho).png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/SplashScene(Mytho).png",
      "resizeMode": "contain",
      "backgroundColor": "#a4dc64"
    },
    "updates": {
      "enabled": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SplashScene(Mytho).png",
        "backgroundColor": "#a4dc64"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
